{
  "name": "ember-arcgis-portal-components",
  "version": "1.2.2",
  "description": "Ember components that help when working with ArcGIS Online/Enterprise items",
  "keywords": [
    "esri",
    "arcgis",
    "arcgisonline",
    "emberjs",
    "ember-addon"
  ],
  "license": "Apache-2.0",
  "author": "Dave Bouwman <dbouwman@esri.com>",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "repository": "https://github.com/Esri/ember-arcgis-portal-components",
  "scripts": {
    "build": "ember build",
    "clean": "rm -rf node_modules bower_components",
    "lint:js": "eslint ./*.js addon addon-test-support app config lib server test-support tests",
    "start": "ember serve",
    "test": "ember test --silent",
    "deploy": "ember github-pages:commit --environment=github --message \"Deploy gh-pages from commit $(git rev-parse HEAD)\";"
  },
  "dependencies": {
    "bower": "1.8.2",
    "ember-cli-babel": "^6.6.0",
    "ember-cli-htmlbars": "^2.0.1",
    "ember-cli-sass": "5.5.1",
    "ember-cli-string-helpers": "1.5.0"
  },
  "devDependencies": {
    "broccoli-asset-rev": "^2.4.5",
    "calcite-bootstrap": "^0.4.4",
    "ember-arcgis-portal-services": "^1.7.1",
    "ember-arcgis-server-services": "^1.4.1",
    "ember-cli": "~2.18.2",
    "ember-cli-active-link-wrapper": "0.4.0",
    "ember-cli-app-version": "^3.0.0",
    "ember-cli-bootstrap-sassy": "0.5.5",
    "ember-cli-dependency-checker": "^2.0.0",
    "ember-cli-eslint": "4.2.1",
    "ember-cli-github-pages": "0.2.0",
    "ember-cli-htmlbars-inline-precompile": "^1.0.0",
    "ember-cli-inject-live-reload": "^1.4.1",
    "ember-cli-qunit": "^4.3.2",
    "ember-cli-release": "^0.2.9",
    "ember-cli-shims": "^1.2.0",
    "ember-cli-sri": "^2.1.0",
    "ember-cli-uglify": "^2.0.0",
    "ember-code-snippet": "1.8.1",
    "ember-export-application-global": "^2.0.0",
    "ember-fetch": "^3.4.5",
    "ember-intl": "2.31.1",
    "ember-load-initializers": "^1.0.0",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-radio-button": "1.2.4",
    "ember-resolver": "^4.0.0",
    "ember-sanitize": "2.0.2",
    "ember-source": "~2.18.0",
    "eslint-config-semistandard": "~7.0.0",
    "eslint-config-standard": "~6.0.0",
    "eslint-plugin-ember": "^5.0.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^2.0.1",
    "eslint-plugin-react": "^6.2.0",
    "eslint-plugin-standard": "^2.0.0",
    "loader.js": "^4.2.3",
    "torii": "0.10.1",
    "torii-provider-arcgis": "^2.0.0"
  },
  "engines": {
    "node": "^4.5 || 6.* || >= 7.*"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
}
