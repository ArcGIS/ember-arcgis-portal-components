<a href="" {{action 'selectItem' model}}>
  <div class="checkbox-inline">
    <input type="checkbox" checked={{readonly checked}}>
  </div>
  <div class="item-picker-item-results-item-inner">
    <h2 data-toggle="tooltip" title="{{model.title}}">
      {{model.title}}
    </h2>
    <div class="shared-by">
      {{model.owner}}
    </div>
  </div>
</a>
<a class="pull-right item-picker-item-results-item-details" target="_blank" href={{url}}>{{t (concat _i18nScope "itemDetails")}}</a>
